{
  "name": "amis-editor-deploy",
  "version": "1.0.0",
  "description": "使用react-app-rewired打包amis-editor，方便部署",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "amis": "^1.8.0",
    "amis-editor": "^4.1.0-beta.4",
    "antd": "^4.19.0",
    "axios": "^0.21.1",
    "copy-to-clipboard": "^3.2.0",
    "mobx": "^4.5.0",
    "mobx-react": "^6.1.4",
    "mobx-state-tree": "^3.17.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "5.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-app-rewired start --REACT_APP_ENV=local",
    "build:prod": "react-app-rewired build --REACT_APP_ENV=prod",
    "build:test": "react-app-rewired build --REACT_APP_ENV=test",
    "build:dev": "react-app-rewired build --REACT_APP_ENV=dev",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.2",
    "copy-webpack-plugin": "^10.2.4",
    "customize-cra": "^1.0.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-app-rewire-html-webpack-plugin": "^1.1.0",
    "react-app-rewired": "^2.2.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rencoo/amis-editor-deploy.git"
  },
  "keywords": [
    "amis-editor"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rencoo/amis-editor-deploy/issues"
  }
}
